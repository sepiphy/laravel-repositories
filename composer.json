{
    "name": "sepiphy/laravel-repositories",
    "description": "The Sepiphy Laravel Repositories package.",
    "keywords": [
        "sepiphy", "laravel", "laravel-repositories", "repositories"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/laravel-extensions",
    "support": {
        "issues": "https://github.com/sepiphy/laravel-extensions/issues",
        "source": "https://github.com/sepiphy/laravel-extensions"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com",
            "homepage": "https://seriquynh.com",
            "role": "Software Developer"
        }
    ],
    "require": {
        "illuminate/support": "5.8.*",
        "illuminate/database": "5.8.*",
        "sepiphy/laravel-support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Laravel\\Repositories\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Sepiphy\\Laravel\\Repositories\\RepositoryServiceProvider"
            ]
        }
    },
    "suggest": {
        "sepiphy/laravel-contracts": "Required to use the sepiphy repository interface (^0.3)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
