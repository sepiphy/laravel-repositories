{
    "name": "sepiphy/laravel-repositories",
    "description": "The sepiphy/laravel-repositories package",
    "keywords": [
        "sepiphy", "laravel", "laravel-repositories", "repositories"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/laraveltools",
    "support": {
        "issues": "https://github.com/sepiphy/laraveltools/issues",
        "source": "https://github.com/sepiphy/laraveltools"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "illuminate/support": "5.8.*",
        "illuminate/database": "5.8.*",
        "sepiphy/laravel-support": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Laravel\\Repositories\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Sepiphy\\Laravel\\Repositories\\RepositoryServiceProvider"
            ]
        }
    },
    "suggest": {
        "sepiphy/laravel-contracts": "Required to use the sepiphy repository interface (^0.3)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
